<h1>Pokemon:</h1>
{{#each foo}}
{{this}}
{{/each}}
<table>
    <thead>
        <th>ID</th>
        <th>Name</th>
    </thead>
    <tbody> 
        {{#each pokemon}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td><button onclick="deletePoke({{id}})">Delete</button></td>
            <td><a href="/pokemon/{{id}}">Update</a></td>
        </tr> 
        {{/each}} 
    </tbody> 
    <form id="addpoke" action="/pokemon" method="post">
        ID:  <input type="number" name="id"><br>
        Name: <input type="text" name="name"><br>
        <input type="submit" value="Submit">
    </form>
</table> 
<br>
<a href="/">Return to homepage</a>
