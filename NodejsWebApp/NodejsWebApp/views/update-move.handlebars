<h1>Update {{move.name}}:</h1>
<form id="update-move" action="/moves" method="post">
    Name: <input type="text" name="name" value="{{move.name}}"><br>
    Type: <select name="tid" id="type-selector">
        {{#each type}}
        <option value="{{id}}">{{name}}</option>
        {{/each}}
    </select><br>
</form>
<button onclick="updateMove({{move.id}})">Update</button>
<script defer>selectType({{move.tid}});</script>

